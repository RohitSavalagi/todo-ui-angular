{
  "name": "todo-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run todo-ui:serve-ssr",
    "serve:ssr:todo-ui": "node dist/todo-ui/server/server.mjs",
    "prettier:write": "prettier --config .prettierrc --write .",
    "prettier:check": "prettier --config .prettierrc --check .",
    "lint": "eslint --color --quiet -c eslint.config.mjs ./src/app",
    "fix-lint": "eslint --color --fix --quiet -c eslint.config.mjs ./src/app",
    "fix-style": "stylelint --formatter verbose less \"src/{environments,styles}/**/*.less\" --fix"
  },
  "private": true,
  "dependencies": {
    "-": "^0.0.1",
    "@angular-eslint/eslint-plugin": "^20.2.0",
    "@angular-eslint/eslint-plugin-template": "^20.2.0",
    "@angular-eslint/template-parser": "^20.2.0",
    "@angular/animations": "^20.2.3",
    "@angular/cdk": "^20.2.2",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/material": "^20.2.2",
    "@angular/platform-browser": "^20.2.0",
    "@angular/platform-server": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@angular/ssr": "^20.2.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^8.42.0",
    "bootstrap": "^5.3.8",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "save-dev": "^0.0.1-security",
    "tslib": "^2.3.0",
    "tslint": "v5.18",
    "typescript-eslint": "^8.42.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.0",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@stylistic/stylelint-plugin": "^3.1.1",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "less": "^4.2.0",
    "prettier": "3.6.2",
    "sass": "^1.92.1",
    "typescript": "~5.9.2"
  }
